{
  "name": "stack-overflow-clone-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "pnpm build:storybook && next build",
    "vercel-build": "pnpm prisma:generate && prisma db push && pnpm build",
    "start": "next start",
    "lint": "next lint",
    "dev:storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build -o ./public/storybook-static",
    "test": "vitest",
    "prisma:studio": "dotenv -e .env.local -- pnpm prisma studio",
    "prisma:generate": "pnpm prisma generate",
    "prisma:push": "dotenv -e .env.local -- pnpm prisma db push",
    "gen:git-log-file": "mkdir -p modules/shared/assets && git log -1 --pretty=format:\"%cd %h %s\" > modules/shared/assets/git.log",
    "prepare": "husky & tsx _scripts/prepare & pnpm gen:git-log-file"
  },
  "prisma": {
    "schema": "modules/prisma/schema.prisma"
  },
  "dependencies": {
    "@clerk/nextjs": "^5.1.3",
    "@hookform/resolvers": "^3.5.0",
    "@prisma/client": "^5.15.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-menubar": "^1.0.4",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@storybook/manager-api": "^8.1.5",
    "@storybook/preview-api": "^8.1.5",
    "@toast-ui/editor": "^3.2.2",
    "@toast-ui/editor-plugin-code-syntax-highlight": "^3.1.0",
    "@toast-ui/editor-plugin-color-syntax": "^3.1.0",
    "@toast-ui/react-editor": "^3.2.3",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash-es": "^4.17.12",
    "@vercel/postgres": "^0.8.0",
    "ahooks": "^3.8.0",
    "change-case": "^5.4.4",
    "chokidar": "^3.6.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv-cli": "^7.4.2",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "husky": "^9.0.11",
    "javascript-time-ago": "^2.5.10",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.379.0",
    "next": "14.2.3",
    "prisma": "^5.15.0",
    "prismjs": "^1.29.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.5",
    "react-markdown": "^9.0.1",
    "tailwind-merge": "^2.3.0",
    "ts-dedent": "^2.2.0",
    "tsx": "^4.11.2",
    "type-fest": "^4.19.0",
    "vercel": "^34.2.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.5.0",
    "@storybook/addon-essentials": "^8.1.5",
    "@storybook/addon-interactions": "^8.1.5",
    "@storybook/addon-links": "^8.1.5",
    "@storybook/addon-onboarding": "^8.1.5",
    "@storybook/addon-storysource": "^8.1.5",
    "@storybook/blocks": "^8.1.5",
    "@storybook/nextjs": "^8.1.5",
    "@storybook/react": "^8.1.5",
    "@storybook/test": "^8.1.5",
    "@tailwindcss/typography": "^0.5.13",
